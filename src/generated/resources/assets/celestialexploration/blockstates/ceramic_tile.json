{
  "multipart": [
    {
      "apply": {
        "model": "celestialexploration:block/ceramic_tile_post"
      }
    },
    {
      "when": {
        "north": "true"
      },
      "apply": {
        "model": "celestialexploration:block/ceramic_tile_side"
      }
    },
    {
      "when": {
        "north": "false"
      },
      "apply": {
        "model": "celestialexploration:block/ceramic_tile_noside"
      }
    },
    {
      "when": {
        "south": "true"
      },
      "apply": {
        "model": "celestialexploration:block/ceramic_tile_side_alt"
      }
    },
    {
      "when": {
        "south": "false"
      },
      "apply": {
        "model": "celestialexploration:block/ceramic_tile_noside_alt",
        "y": 90
      }
    },
    {
      "when": {
        "west": "true"
      },
      "apply": {
        "model": "celestialexploration:block/ceramic_tile_side_alt",
        "y": 90
      }
    },
    {
      "when": {
        "west": "false"
      },
      "apply": {
        "model": "celestialexploration:block/ceramic_tile_noside",
        "y": 270
      }
    },
    {
      "when": {
        "east": "true"
      },
      "apply": {
        "model": "celestialexploration:block/ceramic_tile_side",
        "y": 90
      }
    },
    {
      "when": {
        "east": "false"
      },
      "apply": {
        "model": "celestialexploration:block/ceramic_tile_noside_alt"
      }
    }
  ]
}