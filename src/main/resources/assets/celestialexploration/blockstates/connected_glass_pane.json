{
  "multipart": [
    {
      "apply": {
        "model": "celestialexploration:block/connected_glass_pane_post"
      }
    },
    {
      "when": {
        "north": "true"
      },
      "apply": {
        "model": "celestialexploration:block/connected_glass_pane_side"
      }
    },
    {
      "when": { "AND": [
        { "north": "true", "up": "false" }
        
      ]},
      "apply": {
        "model": "celestialexploration:block/connected_glass_pane_top"
      }
    },
    {
      "when": { "AND": [
        { "north": "true", "down": "false" }
        
      ]},
      "apply": {
        "model": "celestialexploration:block/connected_glass_pane_top",
        "x": 180
      }
    },
    {
      "when": {
        "east": "true"
      },
      "apply": {
        "model": "celestialexploration:block/connected_glass_pane_side",
        "y": 90
      }
    },
    {
      "when": {
        "south": "true"
      },
      "apply": {
        "model": "celestialexploration:block/connected_glass_pane_side_alt"
      }
    },
    {
      "when": {
        "west": "true"
      },
      "apply": {
        "model": "celestialexploration:block/connected_glass_pane_side_alt",
        "y": 90
      }
    },
    {
      "when": {
        "north": "false"
      },
      "apply": {
        "model": "celestialexploration:block/connected_glass_pane_noside"
      }
    },
    {
      "when": {
        "east": "false"
      },
      "apply": {
        "model": "celestialexploration:block/connected_glass_pane_noside_alt"
      }
    },
    {
      "when": {
        "south": "false"
      },
      "apply": {
        "model": "celestialexploration:block/connected_glass_pane_noside_alt",
        "y": 90
      }
    },
    {
      "when": {
        "west": "false"
      },
      "apply": {
        "model": "celestialexploration:block/connected_glass_pane_noside",
        "y": 270
      }
    }
  ]
}